    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "postcss-preset-env": "^8.3.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.8.3"
  }
